{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "lint",
		"type": "shell",
		"command": "npm run lint",
		"problemMatcher": ["$eslint-stylish"],
		"group": {
		  "kind": "build",
		  "isDefault": false
		},
		"options": {
		  "cwd": "${workspaceFolder}"
		}
	  },
	  {
		"label": "build",
		"type": "shell",
		"command": "npm run build",
		"problemMatcher": [],
		"group": {
		  "kind": "build",
		  "isDefault": false
		},
		"options": {
		  "cwd": "${workspaceFolder}"
		}
	  },
	  {
		"label": "lint-and-build",
		"dependsOrder": "sequence",
		"dependsOn": ["lint", "build"],
		"group": {
		  "kind": "build",
		  "isDefault": true
		}
	  }
	]
  }
  