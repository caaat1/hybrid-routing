{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "dictionaries": [
    "custom-words"
  ],
  "dictionaryDefinitions": [
    {
      "addWords": true,
      "name": "custom-words",
      "path": "./custom-words.txt"
    }
  ],
  "ignorePaths": [
    "node_modules/**",
    "dist/**",
    "build/**",
    ".git/**",
    "public/**",
    "**/*/custom-words.txt",
    "**/*/custom-dictionary-folder.txt"
  ],
  "ignoreWords": [],
  "import": [],
  "language": "en",
  "version": "0.2",
  "words": [
    "chownr",
    "depd",
    "dunder",
    "encodeurl",
    "finalhandler",
    "fullwidth",
    "gopd",
    "hasown",
    "libc",
    "minimatch",
    "minipass",
    "minizlib",
    "mkdirp",
    "nopt",
    "npmlog",
    "parseurl",
    "realpath",
    "rimraf",
    "setprototypeof",
    "toidentifier",
    "typer",
    "unpipe",
    "weakmap",
    "webidl",
    "whatwg",
    "wrappy",
    "yallist"
  ]
}